		<property name="restrictionTypeCode" value="CMSUserGroupRestriction" />
		<property name="restrictionEvaluator" ref="cmsUserGroupRestrictionEvaluator" />
	</bean>

	<!-- dynamic attributes -->
	<bean id="AbstractRestriction_descriptionAttributeHandler" class="de.hybris.platform.cms2.model.AbstractRestrictionDescriptionAttributeHandler" />
