	<bean id="productRestrictionDynamicDescription" class="de.hybris.platform.cms2.model.ProductRestrictionDescription" />
	<bean id="timeRestrictionDynamicDescription" class="de.hybris.platform.cms2.model.TimeRestrictionDescription" />
	<bean id="userRestrictionDynamicDescription" class="de.hybris.platform.cms2.model.UserRestrictionDescription" />
	<bean id="userGroupRestrictionDynamicDescription" class="de.hybris.platform.cms2.model.UserGroupRestrictionDescription" />

	<alias name="abstractPageDynamicNavigationNodeList" alias="abstractPagenaDynamicNavigationNodeList"/>
	<bean id="abstractPageDynamicNavigationNodeList" class="de.hybris.platform.cms2.model.AbstractPageDynamicNavigationNodeList">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
		<property name="modelService" ref="modelService"/>
		<property name="cmsNavigationEntryCodeGenerator">
			<bean class="de.hybris.platform.servicelayer.keygenerator.impl.PersistentKeyGenerator">
				<property name="key" value="${keygen.navigationentry.uid.key}"/>
				<property name="digits" value="${keygen.navigationentry.uid.digits}"/>
				<property name="start" value="${keygen.navigationentry.uid.start}"/>
				<property name="numeric" value="${keygen.navigationentry.uid.numeric}"/>
			</bean>
		</property>
	</bean>

	<!--  Strategies & Registries -->
	<alias alias="activateBaseSiteInSessionStrategy" name="cms2ActivateBaseSiteInSessionStrategy"/>
	<bean id="cms2ActivateBaseSiteInSessionStrategy"
			class="de.hybris.platform.cms2.strategies.impl.DefaultCMSActivateBaseSiteInSessionStrategy">
		<property name="catalogVersionService" ref="catalogVersionService"/>
		<lookup-method name="lookupCmsSiteService" bean="cmsSiteService"/>
	</bean>

	<alias alias="cmsComponentContainerRegistry" name="defaultCMS2ComponentContainerRegistry" />
	<bean id="defaultCMS2ComponentContainerRegistry" class="de.hybris.platform.cms2.registry.impl.DefaultCMSComponentContainerRegistry">
		<property name="defaultStrategy" ref="cms2ComponentContainerStrategy" />
	</bean>

	<alias alias="cms2ComponentContainerStrategy" name="defaultCMS2ComponentContainerStrategy" />
	<bean id="defaultCMS2ComponentContainerStrategy" class="de.hybris.platform.cms2.strategies.impl.DefaultCMSComponentContainerStrategy" />
	
	<!-- Sort Query Registries -->
	<alias name="defaultSortQueryDataRegistry" alias="cmsSortQueryDataRegistry" />
	<bean id="defaultSortQueryDataRegistry" class="de.hybris.platform.cms2.services.impl.DefaultSortQueryDataRegistry" />

	<bean class="de.hybris.platform.cms2.services.impl.DefaultSortQueryData" 
			p:typeClass="de.hybris.platform.cms2.servicelayer.daos.impl.DefaultCMSComponentDao"
			p:sortCode="name"
			p:query="ORDER BY LOWER({name}) ASC, {pk}"
			p:default="false" />
			
	<bean class="de.hybris.platform.cms2.services.impl.DefaultSortQueryData" 
			p:typeClass="de.hybris.platform.cms2.servicelayer.daos.impl.DefaultCMSComponentDao"
			p:sortCode="modifiedtime"
			p:query="ORDER BY {modifiedtime} DESC, {pk}"
			p:default="true" />
			
</beans>
