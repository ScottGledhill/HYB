	</bean>

	<!-- Synchronization Validators -->
	<alias name="cmsCreateCatalogSynchronizationValidator" alias="createCatalogSynchronizationValidator" />
	<bean id="cmsCreateCatalogSynchronizationValidator" class="de.hybris.platform.cmsfacades.synchronization.validator.CreateCatalogSynchronizationValidator">
		<property name="userService" ref="userService" />
		<property name="catalogVersionService" ref="catalogVersionService" />
	</bean>

	<alias name="defaultSyncJobRequestCompositeValidator" alias="syncJobRequestCompositeValidator" />
	<bean id="defaultSyncJobRequestCompositeValidator" class="de.hybris.platform.cmsfacades.common.validator.CompositeValidator">
		<property name="validators">
			<list merge="true">
				<ref bean="defaultSyncJobRequestValidator" />
			</list>
		</property>
	</bean>

	<alias name="cmsCreateCatalogSynchronizationCompositeValidator" alias="createCatalogSynchronizationCompositeValidator" />
	<bean id="cmsCreateCatalogSynchronizationCompositeValidator" class="de.hybris.platform.cmsfacades.common.validator.CompositeValidator">
		<property name="validators">
			<list merge="true">
				<ref bean="defaultSyncJobRequestValidator" />
				<ref bean="createCatalogSynchronizationValidator" />
			</list>
		</property>
	</bean>

	<alias name="defaultComponentTypeFacade" alias="componentTypeFacade" />
	<bean id="defaultComponentTypeFacade" class="de.hybris.platform.cmsfacades.types.impl.DefaultComponentTypeFacade">
		<property name="componentTypeStructureRegistry" ref="componentTypeStructureRegistry" />
		<property name="componentTypeStructureConverter" ref="componentTypeStructureConverter" />
		<property name="typeService" ref="typeService" />
		<property name="cmsSupportedAbstractTypecodes" ref="cmsSupportedAbstractTypecodes" />
	</bean>

	<alias name="defaultPageContentSlotTypeRestrictionsFacade" alias="pageContentSlotTypeRestrictionsFacade" />
