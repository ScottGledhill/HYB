	<bean id="abstractCatalogVersionDetailsPopulator" class="de.hybris.platform.cmsfacades.catalogversiondetails.populator.AbstractCatalogVersionModelPopulator" abstract="true">
		<property name="localizedPopulator" ref="cmsLocalizedPopulator" />
