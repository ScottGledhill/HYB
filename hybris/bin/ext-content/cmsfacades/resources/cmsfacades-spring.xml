	<bean id="defaultContentCatalogVersionDetailsListPopulator" class="de.hybris.platform.cmsfacades.catalogversiondetails.populator.ContentCatalogVersionModelPopulator" parent="abstractCatalogVersionDetailsPopulator"></bean>

	<alias name="defaultProductCatalogVersionDetailsListPopulator" alias="productCatalogVersionDetailsListPopulator" />
	<bean id="defaultProductCatalogVersionDetailsListPopulator" class="de.hybris.platform.cmsfacades.catalogversiondetails.populator.ProductCatalogVersionModelPopulator" parent="abstractCatalogVersionDetailsPopulator">
		<property name="baseSiteService" ref="baseSiteService" />
	</bean>

	<alias name="defaultCatalogVersionDetailDataComparator" alias="catalogVersionDetailDataComparator" />
	<bean id="defaultCatalogVersionDetailDataComparator" class="de.hybris.platform.cmsfacades.catalogversiondetails.CatalogVersionDetailDataCatalogIdComparator" />


	<!-- Comparator -->
	<alias name="itemModifiedTimeComparator" alias="cmsItemComparator" />
	<bean id="itemModifiedTimeComparator" class="de.hybris.platform.cmsfacades.items.comparator.ItemModifiedTimeComparator" />
	
	<alias name="pageNameComparator" alias="cmsPageComparator" />
	<bean id="pageNameComparator" class="de.hybris.platform.cmsfacades.pages.comparator.PageNameComparator" />

	<!-- Service Strategies -->
	<alias name="defaultSyncJobConverter" alias="syncJobConverter" />
	<bean id="defaultSyncJobConverter" parent="abstractPopulatingConverter">
		<property name="targetClass" value="de.hybris.platform.cmswebservices.data.SyncJobData" />
		<property name="populators">
			<list merge="true">
				<ref bean="syncJobDataPopulator" />
			</list>
		</property>
	</bean>

	<bean id="syncJobDataPopulator" class="de.hybris.platform.cmsfacades.synchronization.populator.SyncJobDataPopulator" />


	<!-- NamedQueries -->

	<util:map id="namedQueryMap">
		<entry key="namedQueryMediaSearchByCodeCatalogVersion" value="SELECT {pk} FROM {Media} WHERE LOWER({code}) LIKE LOWER(?code) AND {catalogVersion} = ?catalogVersion" />
		<entry key="namedQueryRestrictionSearchByName" value="SELECT {pk} FROM {AbstractRestriction} WHERE LOWER({name}) LIKE LOWER(?name) AND {catalogVersion} = ?catalogVersion AND {itemtype} = ?itemtype" />
		<entry key="namedQueryProductCategoryByText" value="SELECT {pk} FROM {Category} WHERE ( LOWER({name}) LIKE LOWER(?name) OR LOWER({description}) LIKE LOWER(?description) OR LOWER({code}) LIKE LOWER(?code)) AND {catalogVersion} = ?catalogVersion" />
		<entry key="namedQueryProductByText" value="SELECT {pk} FROM {Product} WHERE ( LOWER({name}) LIKE LOWER(?name) OR LOWER({description}) LIKE LOWER(?description) OR LOWER({code}) LIKE LOWER(?code)) AND {catalogVersion} = ?catalogVersion" />
	</util:map>

	<alias alias="namedQueryFactory" name="defaultNamedQueryFactory" />
	<bean id="defaultNamedQueryFactory" class="de.hybris.platform.cmsfacades.namedquery.service.impl.DefaultNamedQueryFactory">
		<property name="namedQueryMap" ref="namedQueryMap" />
	</bean>

	<alias alias="flexibleSearchNamedQueryConverter" name="defaultFlexibleSearchNamedQueryConverter" />
	<bean id="defaultFlexibleSearchNamedQueryConverter" class="de.hybris.platform.cmsfacades.namedquery.service.impl.FlexibleSearchNamedQueryConverter">
		<property name="defaultPageSize" value="${cmsfacades.namedquery.default.pagesize}" />
	</bean>

	<alias alias="flexibleSearchNamedQueryService" name="defaultFlexibleSearchNamedQueryService" />
	<bean id="defaultFlexibleSearchNamedQueryService" class="de.hybris.platform.cmsfacades.namedquery.service.impl.FlexibleSearchNamedQueryService">
		<property name="flexibleSearchService" ref="flexibleSearchService" />
		<property name="namedQueryFactory" ref="namedQueryFactory" />
		<property name="flexibleSearchNamedQueryConverter" ref="flexibleSearchNamedQueryConverter" />
	</bean>
	
	<alias name="abstractNamedQueryDataPopulator" alias="cmsAbstractNamedQueryDataPopulator" />
	<bean id="abstractNamedQueryDataPopulator" class="de.hybris.platform.cmsfacades.common.populator.impl.AbstractNamedQueryDataPopulator" abstract="true">
		<property name="cmsAdminSiteService" ref="cmsAdminSiteService" />
	</bean>

	<!-- Media Search -->

	<bean id="basicNamedQueryDataPopulator" class="de.hybris.platform.cmsfacades.media.populator.BasicNamedQueryDataPopulator" />

	<bean id="mediaSearchByCodeNamedQueryDataPopulator" parent="cmsAbstractNamedQueryDataPopulator" 
		class="de.hybris.platform.cmsfacades.media.populator.MediaSearchByCodeNamedQueryDataPopulator">
		<property name="catalogVersionService" ref="catalogVersionService" />
	</bean>

	<alias alias="mediaNamedQueryConverter" name="defaultMediaNamedQueryConverter" />
	<bean id="defaultMediaNamedQueryConverter" parent="abstractPopulatingConverter">
		<property name="targetClass" value="de.hybris.platform.cmswebservices.namedquery.NamedQuery" />
		<property name="populators">
			<list merge="true">
				<ref bean="basicNamedQueryDataPopulator" />
				<ref bean="mediaSearchByCodeNamedQueryDataPopulator" />
			</list>
		</property>
	</bean>

	<alias alias="cmsEnumDataFacade" name="enumDataFacade"/>
	<bean id="enumDataFacade" class="de.hybris.platform.cmsfacades.enumdata.impl.DefaultEnumDataFacade">
		<property name="enumerationService" ref="enumerationService"/>
		<property name="enumDataConverter" ref="enumDataConverter"/>
	</bean>

	<alias alias="enumDataConverter" name="defaultEnumDataConverter" />
	<bean id="defaultEnumDataConverter" class="de.hybris.platform.cmsfacades.enumdata.impl.EnumDataConverter"/>
	
	<!-- Media Format -->
	
	<alias alias="cmsMediaFormatService" name="defaultCmsMediaFormatService" />
	<bean id="defaultCmsMediaFormatService" class="de.hybris.platform.cmsfacades.media.service.impl.DefaultCMSMediaFormatService">
		<property name="cmsComponentMediaFormats" ref="cmsComponentMediaFormats" />
		<property name="cmsMediaFormatDao" ref="cmsMediaFormatDao" />
	</bean>
		
	<util:map id="cmsComponentMediaFormats" key-type="java.lang.Class">
		<entry key="de.hybris.platform.acceleratorcms.model.components.AbstractMediaContainerComponentModel" 
				 value-ref="cmsMediaContainerComponentModelMediaFormats" />
	</util:map>
	
	<util:list id="cmsMediaContainerComponentModelMediaFormats">
		<value>widescreen</value>
		<value>desktop</value>
		<value>tablet</value>
		<value>mobile</value>
	</util:list>

