	<alias name="defaultCatalogVersionDetailDataFacade" alias="catalogVersionDetailsFacade" />
	<bean id="defaultCatalogVersionDetailDataFacade" class="de.hybris.platform.cmsfacades.catalogversiondetails.impl.DefaultCatalogVersionDetailDataFacade">
