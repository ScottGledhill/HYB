	<alias name="defaultCmsSiteFacade" alias="siteFacade" />
	<bean id="defaultCmsSiteFacade" class="de.hybris.platform.cmsfacades.sites.impl.DefaultSiteFacade">
		<property name="cmsAdminSiteService" ref="cmsAdminSiteService" />
