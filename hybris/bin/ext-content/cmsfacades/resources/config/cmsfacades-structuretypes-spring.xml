		<property name="populators">
			<set>
				<ref bean="basicComponentTypeAttributePopulator" />
				<ref bean="i18nComponentTypeAttributePopulator" />
				<ref bean="multiProductSelectorPopulator" />
			</set>
		</property>
	</bean>
	<bean class="de.hybris.platform.cmsfacades.types.service.impl.DefaultComponentTypeAttributeStructure"  p:typecode="ProductCarouselComponent" p:qualifier="categories">
		<property name="populators">
			<set>
				<ref bean="basicComponentTypeAttributePopulator" />
				<ref bean="i18nComponentTypeAttributePopulator" />
